import os
import sys
from functools import reduce

module_dir = os.path.dirname(os.path.abspath(__file__))
project_dir = os.path.abspath(os.path.join(module_dir, '..'))
sys.path.append(project_dir)

from my_project import dijkstras

def test_dijkstras():
    weighted_graph = {
        'A': {'B': 1, 'C': 4},
        'B': {'A': 1, 'C': 2, 'D': 5},
        'C': {'A': 4, 'B': 2, 'D': 1},
        'D': {'B': 5, 'C': 1}
    }

    assert dijkstras(weighted_graph, 'A', 'B') == ['A', 'B']

    assert dijkstras(weighted_graph, 'A', 'C') == ['A', 'C']

    assert dijkstras(weighted_graph, 'A', 'D') == ['A', 'C', 'D']

    assert dijkstras(weighted_graph, 'B', 'C') == ['B', 'C']

    assert dijkstras(weighted_graph, 'C', 'D') == ['C', 'D']

    assert dijkstras(weighted_graph, 'B', 'D') == ['B', 'C', 'D']

    assert dijkstras(weighted_graph, 'D', 'A') == []

    assert dijkstras(weighted_graph, 'C', 'A') == []

    assert dijkstras(weighted_graph, 'D', 'B') == []

    assert dijkstras(weighted_graph, 'C', 'B') == ['C', 'B']