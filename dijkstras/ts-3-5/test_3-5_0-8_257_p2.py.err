import os
import sys

module_dir = os.path.dirname(os.path.abspath(__file__))
project_dir = os.path.abspath(os.path.join(module_dir, '..'))
sys.path.append(project_dir)

from dijkstras import dijkstras

def test_dijkstras():
    weighted_graph = {
        'A': {'B': 7, 'C': 4},
        'B': {'A': 7, 'C': 2, 'D': 3},
        'C': {'A': 4, 'B': 2, 'D': 5},
        'D': {'B': 3, 'C': 5}
    }

    src = 'A'
    dst = 'D'

    result = dijkstras(weighted_graph, src, dst)
    assert result == ['A', 'C', 'D']

def test_dijkstras_multiple_paths():
    weighted_graph = {
        'A': {'B': 2, 'C': 4},
        'B': {'A': 2, 'C': 1, 'D': 2},
        'C': {'A': 4, 'B': 1, 'D': 3},
        'D': {'B': 2, 'C': 3}
    }

    src = 'A'
    dst = 'D'

    result = dijkstras(weighted_graph, src, dst)
    assert result == ['A', 'B', 'D']

def test_dijkstras_no_path():
    weighted_graph = {
        'A': {'B': 2},
        'B': {'A': 2, 'C': 1},
        'C': {'B': 1}
    }

    src = 'A'
    dst = 'C'

    result = dijkstras(weighted_graph, src, dst)
    assert result == []

def test_dijkstras_same_source_and_destination():
    weighted_graph = {
        'A': {'B': 2}
    }

    src = 'A'
    dst = 'A'

    result = dijkstras(weighted_graph, src, dst)
    assert result == ['A']